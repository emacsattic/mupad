	  list='$(nobase_dist_lisp_LISP)'; for p in $$list; do \
	    $(am__vpath_adj) \
	    echo " rm -f '$(DESTDIR)$(lispdir)/$$f' '$(DESTDIR)$(lispdir)/$${f}c'"; \
