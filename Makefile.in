elc-stamp: $(LISP)
	@echo 'WARNING: Warnings can be ignored. :-)'
	@rm -f elc-temp && touch elc-temp
	if test "$(EMACS)" != no; then \
	  set x; \
	  list='$(LISP)'; for p in $$list; do \
	    if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	    set x "$$@" "$$d$$p"; shift; \
	  done; \
	  shift; \
	  EMACS="$(EMACS)" $(SHELL) $(elisp_comp) "$$@" || exit 1; \
	else : ; fi
	@mv -f elc-temp $@
$(am__ELCFILES): elc-stamp
	@if test ! -f $@; then \
	  rm -f elc-stamp; \
	  $(MAKE) $(AM_MAKEFLAGS) elc-stamp; \
	else : ; fi
install-nobase_dist_lispLISP: $(nobase_dist_lisp_LISP) $(ELCFILES)
	@$(NORMAL_INSTALL)
	@if test "$(EMACS)" != no; then \
	  test -z "$(lispdir)" || $(mkdir_p) "$(DESTDIR)$(lispdir)"; \
	  $(am__vpath_adj_setup) \
	  list='$(nobase_dist_lisp_LISP)'; for p in $$list; do \
	    if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	    $(am__vpath_adj) \
	    echo " $(nobase_dist_lispLISP_INSTALL) '$$d$$p' '$(DESTDIR)$(lispdir)/$$f'"; \
	    $(nobase_dist_lispLISP_INSTALL) "$$d$$p" "$(DESTDIR)$(lispdir)/$$f"; \
	    if test -f $${p}c; then \
	      echo " $(nobase_dist_lispLISP_INSTALL) '$${p}c' '$(DESTDIR)$(lispdir)/$${f}c'"; \
	      $(nobase_dist_lispLISP_INSTALL) "$${p}c" "$(DESTDIR)$(lispdir)/$${f}c"; \
	    else : ; fi; \
	  done; \
	else : ; fi

uninstall-nobase_dist_lispLISP:
