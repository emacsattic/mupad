# Yes, $(DESTDIR) is missing in test -d $$dir.
# This is voluntary so that when building the rpm package, the
# initialization file gets properly installed $(DESTDIR)/$$dir
# coherently with actual host's site-start.d dir.
# I hope this won't break something else.
install-data-local:
	@echo Finalizing the variable substitution in site-start.d/mupacs.el;
	@sed site-start.d/mupacs.el.in -e 's,$${datadir},${datadir},' > site-start.d/mupacs.el
	@found=no;							\
	for dir in $(KNOWN_EMACS_SITE_START_DIRECTORIES); do		\
	  if test -d $$dir; then					\
		found=yes;						\
		echo Installing the emacs startup file site-start.d/mupacs.el in $(DESTDIR)$$dir; \
		$(INSTALL) -d $(DESTDIR)$$dir;	\
		$(INSTALL_DATA) $(srcdir)/site-start.d/mupacs.el $(DESTDIR)$$dir;	\
		break;							\
	  fi;								\
	done;								\
	if [ $$found = no ]; then					\
	    echo "WARNING: Emacs site-wide startup directory site-start.d not found.";\
	    echo "You may want to add by hand the contents of site-start.d/mupacs.el to the site-wide emacs startup file site-start.el";				\
	fi

uninstall-local:
	echo coucou $(DESTDIR)
	for dir in $(KNOWN_EMACS_SITE_START_DIRECTORIES); do		\
	  if test -d $$dir; then					\
		rm $(DESTDIR)$$dir/mupacs.el;				\
		break;							\
	  fi;								\
	done

packages/rpm/SPECS/$(PKG_RPM_NAME).spec: $(PKG_RPM_NAME).spec
	rm -f $@
	cd packages/rpm/SPECS && $(LN_S) ../../../$(PKG_RPM_NAME).spec .

packages/rpm/SOURCES/$(distdir).tar.gz: $(distdir).tar.gz
	rm -f $@
	cd packages/rpm/SOURCES && $(LN_S) ../../../$(distdir).tar.gz .

$(pkg_source).tar.gz:
	make dist

.PHONY: rpm
rpm: packages/rpm/SPECS/$(PKG_RPM_NAME).spec packages/rpm/SOURCES/$(distdir).tar.gz
	rpmbuild --define '_topdir '`pwd`'/packages/rpm'			\
		-ba packages/rpm/SPECS/$(PKG_RPM_NAME).spec		\
		@PKG_SIGN@
	find packages/rpm/RPMS -type f -name '$(PKG_RPM_NAME).*.rpm' -print

.PHONY: release release-internal cvs-tag cvs-tag-force

cvs-tag:
	cvs tag -c @PACKAGE@-$(H_VERSION)

cvs-tag-force:
	cvs tag -c -F @PACKAGE@-$(H_VERSION)

release-internal: cvs-tag dist rpm

release: release-internal
	mkdir -p binaries
	for f in @PACKAGE@-*.tar.* \
		$$(find packages/rpm -type f -name '*.rpm'); do \
		mv $$f binaries/; \
	done

SFweb: $(PKG_DOC)
	rsync -raz $^ --rsh "ssh -v1" $(SFPACKAGE).sourceforge.net:$(SFDIR)

SFrelease:
	ncftpput upload.sourceforge.net /incoming binaries/*

SFcvsroot: $(SFPACKAGE)-cvsroot.tar.gz

.PHONY: FORCE

%-cvsroot.tar.gz: FORCE
	-mv $@ $@.old
	wget http://cvs.sourceforge.net/cvstarballs/$@

